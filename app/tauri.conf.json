{
  "productName": "delegated-spartan",
  "version": "0.1.0",
  "identifier": "ventures.wicked.delegatedspartan",
  "build": {
    "beforeDevCommand": "cd app/ui; npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "cd app/ui; npm run build",
    "frontendDist": "ui/build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Delegeted Spartan Benchmark",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "Remco Bloemen (Personal Team)"
    }
  }
}